{
  "name": "CCC Library",
  "version": "1.0",
  "description": "Christ Community Church Library kiosk software",
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://circ.libraryworld.com/selfservice.html*"],
      "css": ["content-login.css"],
      "js": ["content-login.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": [
        "content-banner.js",
        "content-timeout.js"
      ]
    }
  ],
  "options_page": "options.html",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "web_accessible_resources": [
    "blank.html",
    "icon128.png",
    "logo.png"
  ],
  "manifest_version": 2
}
